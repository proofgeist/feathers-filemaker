{
  "name": "feathers-filemaker",
  "description": "Filemaker adapter for feathers.js",
  "version": "0.6.1",
  "homepage": "https://github.com/geistinteractive/feathers-filemaker",
  "main": "lib/",
  "keywords": [
    "FileMaker",
    "feathers",
    "feathers-plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/geistinteractive/feathers-filemaker.git"
  },
  "author": {
    "name": "Todd Geist",
    "email": "todd@geistinteractive.com",
    "url": "https://geistinteractive.com"
  },
  "contributors": [
    "todd@geistinteractive.com"
  ],
  "bugs": {
    "url": "https://github.com/geistinteractive/feathers-filemaker/issues"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "publish": "git push origin && git push origin --tags",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "compile": "rm -rf lib/ && babel -d lib/ src/",
    "watch": "babel --watch -d lib/ src/",
    "jshint": "jshint src/. test/. --config",
    "mocha": "mocha test/ --compilers js:babel-core/register",
    "test": "npm run compile && npm run mocha"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "basic-auth": "^1.0.4",
    "debug": "^2.2.0",
    "feathers": "^2.0.1",
    "feathers-commons": "^0.7.1",
    "feathers-errors": "^2.0.1",
    "feathers-hooks": "^1.5.2",
    "feathers-query-filters": "^1.1.1",
    "is": "^3.1.0",
    "joi": "^8.0.5",
    "request-promise": "^3.0.0",
    "uberproto": "^1.2.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "body-parser": "^1.14.1",
    "dotenv": "^2.0.0",
    "feathers": "^2.0.1",
    "feathers-rest": "^1.2.2",
    "feathers-service-tests": "^0.5.8",
    "feathers-socketio": "^1.3.3",
    "jshint": "^2.9.2",
    "mocha": "^2.4.5",
    "nsp": "^2.2.0"
  }
}
